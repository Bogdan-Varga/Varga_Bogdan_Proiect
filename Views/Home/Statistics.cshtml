@model IEnumerable<Varga_Bogdan_Proiect.Models.ShopViewModels.OrderGroup>
@{
    ViewData["Denumire"] = "Ordered Costume Statistics";
}
<h2>Ordered Costume Statistics</h2>
<table>
    <tr>
        <th>
            Order Date
        </th>
        <th>
            Ordered Costume
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderDate)
            </td>
            <td>
                @item.CostumCount
            </td>
        </tr>
    }
</table>
